<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:left="5px" android:right="5px">
        <shape android:shape="rectangle">
            <solid android:color="@color/keyboard_divider"/>
        </shape>
    </item>     <!--divider-->

    <item android:bottom="5px" android:top="5px"
          android:left="5px" android:right="5px">
        <shape android:shape="rectangle">
            <corners
                android:radius="@dimen/key_corner"
                android:topLeftRadius="@dimen/key_corner"
                android:topRightRadius="@dimen/key_corner"
                android:bottomLeftRadius="@dimen/key_corner"
                android:bottomRightRadius="@dimen/key_corner" />
            <solid android:color="#4054CA"/>
        </shape>
    </item>

</layer-list>